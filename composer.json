{
    "name": "elbgoods/laravel-nova-activity-log-properties-field",
    "description": "Prettified Field for the properties Attribute of the activity log table.",
    "keywords": [
        "elbgoods",
        "laravel-nova-activity-log-properties-field"
    ],
    "homepage": "https://github.com/elbgoods/laravel-nova-activity-log-properties-field",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Elbgoods GmbH",
            "email": "devteam@elbgoods.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Elbgoods\\LaravelNovaActivityLogPropertiesField\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elbgoods\\LaravelNovaActivityLogPropertiesField\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elbgoods\\LaravelNovaActivityLogPropertiesField\\LaravelNovaActivityLogPropertiesFieldServiceProvider"
            ],
            "aliases": {
                "LaravelNovaActivityLogPropertiesField": "Elbgoods\\LaravelNovaActivityLogPropertiesField\\LaravelNovaActivityLogPropertiesFieldFacade"
            }
        }
    }
}
